#!/usr/bin/env node

const { default: App } = require('../dist/app')
const fastify = require('fastify')({
  logger: true
})

App(fastify, {})
fastify.listen(9000, '0.0.0.0', function (err, address) {
  if (err) {
    fastify.log.error(err)
    process.exit(1)
  }
  fastify.log.info(`server listening on ${address}`)
})
